[
    {
    "key": "Alt+Right",
    "command": "editor.action.insertSnippet",
    "when": "editorTextFocus",
    "args": {
        "snippet": "»"
    }
},
    {
    "key": "Alt+Left",
    "command": "editor.action.insertSnippet",
    "when": "editorTextFocus",
    "args": {
        "snippet": "«"
    }
},
    {   
    "key": "Ctrl+Shift+M",
    "command": "editor.action.insertSnippet",
    "when": "editorTextFocus",
    "args": {
        "snippet": "${TM_CURRENT_WORD/(.*)/${1:? => :=> }/}"
    }
}
]