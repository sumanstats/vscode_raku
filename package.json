{
    "name": "raku",
    "displayName": "raku",
    "description": "Raku programming language support for VS Code",
    "version": "0.5.0",
    "author": "Dr Suman Khanal",
    "license": "MIT",
    "homepage": "https://github.com/sumanstats/vscode_raku",
    "repository": {
        "type": "git",
        "url": "https://github.com/sumanstats/vscode_raku"
    },
    "bugs": {
        "url": "https://github.com/sumanstats/vscode_raku/issues"
    },
    "publisher": "sumankhanal",
    "icon": "./images/icon.png",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Other"
    ],
    "keywords": [
        "Raku",
        "Raku programming language",
        "Raku module",
        "Raku doc"
    ],
    "contributes": {
        "languages": [
            {
                "id": "raku",
                "aliases": [
                    "Raku",
                    "raku"
                ],
                "extensions": [
                    ".nqp",
                    ".raku",
                    ".rakumod",
                    ".rakudoc",
                    ".rakutest",
                    ".t"
                ],
                "firstLine": "(?x) ^ \\s* (?: use \\s v6 | =begin \\s pod | =comment | \\#!(?: (?:perl6|raku)|/.*(?:perl6|raku) (?! \\S)) | my\\s*class )",
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "raku",
                "scopeName": "source.raku",
                "path": "./syntaxes/raku.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "raku",
                "path": "./snippets/raku.json"
            }
        ],
        "keybindings": "keybindings.json",
        "iconThemes": [
            {
                "id": "raku",
                "label": "Raku",
                "path": "./fileicons/raku-icon-theme.json"
            }
        ]
    }
}